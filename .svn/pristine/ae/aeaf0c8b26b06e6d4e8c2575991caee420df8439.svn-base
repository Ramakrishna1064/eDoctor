package com.ensis.mediguru.dto;

import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="treatment")
public class Treatment {

	@Id
	private int treatmentid;
	
	@Column(name="patientid")
	private int patientid;
	
	@Column(name="name")
	private int name;
	
	@Column(name="description")
	private String description;
	
	@Column(name="treatmenttypeid")
	private int  treatmenttypeid;
	
	@Column(name="tretstatustypeid")
	private int  tretstatustypeid;
	
	@Column(name="location")
	private String  location;
	
	@Column(name="physicianid")
	private Integer  physicianid;
	
	@Column(name="specialitytypeid")
	private int  specialitytypeid;
	
	@Column(name="locationradiusfiltervalueid")
	private int  locationradiusfiltervalueid;
	
	@Column(name="timefiltervalueid")
	private int  timefiltervalueid;
	
	@Column(name="costfiltervalueid")
	private int  costfiltervalueid;
	
	@Column(name="reviewfiltervalueid")
	private int  reviewfiltervalueid;
	
	@Column(name="status")
	private int  status;
	
	@Column(name="createdby")
	private String  createdby;
	
	@Column(name="createddate")
	private Date  createddate;
	
	@Column(name="updatedby")
	private String  updatedby;
	
	@Column(name="updateddate")
	private Date  updateddate;
	
	
	public Treatment() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Treatment(int treatmentid, int patientid, int name,
			String description, int treatmenttypeid, int tretstatustypeid,
			String location, int physicianid, int specialitytypeid,
			int locationradiusfiltervalueid, int timefiltervalueid,
			int costfiltervalueid, int reviewfiltervalueid, int status,
			String createdby, Date createddate, String updatedby,
			Date updateddate) {
		super();
		this.treatmentid = treatmentid;
		this.patientid = patientid;
		this.name = name;
		this.description = description;
		this.treatmenttypeid = treatmenttypeid;
		this.tretstatustypeid = tretstatustypeid;
		this.location = location;
		this.physicianid = physicianid;
		this.specialitytypeid = specialitytypeid;
		this.locationradiusfiltervalueid = locationradiusfiltervalueid;
		this.timefiltervalueid = timefiltervalueid;
		this.costfiltervalueid = costfiltervalueid;
		this.reviewfiltervalueid = reviewfiltervalueid;
		this.status = status;
		this.createdby = createdby;
		this.createddate = createddate;
		this.updatedby = updatedby;
		this.updateddate = updateddate;
	}
	public int getTreatmentid() {
		return treatmentid;
	}
	public void setTreatmentid(int treatmentid) {
		this.treatmentid = treatmentid;
	}
	public int getPatientid() {
		return patientid;
	}
	public void setPatientid(int patientid) {
		this.patientid = patientid;
	}
	public int getName() {
		return name;
	}
	public void setName(int name) {
		this.name = name;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public int getTreatmenttypeid() {
		return treatmenttypeid;
	}
	public void setTreatmenttypeid(int treatmenttypeid) {
		this.treatmenttypeid = treatmenttypeid;
	}
	public int getTretstatustypeid() {
		return tretstatustypeid;
	}
	public void setTretstatustypeid(int tretstatustypeid) {
		this.tretstatustypeid = tretstatustypeid;
	}
	public String getLocation() {
		return location;
	}
	public void setLocation(String location) {
		this.location = location;
	}
	public int getPhysicianid() {
		return physicianid;
	}
	public void setPhysicianid(int physicianid) {
		this.physicianid = physicianid;
	}
	public int getSpecialitytypeid() {
		return specialitytypeid;
	}
	public void setSpecialitytypeid(int specialitytypeid) {
		this.specialitytypeid = specialitytypeid;
	}
	public int getLocationradiusfiltervalueid() {
		return locationradiusfiltervalueid;
	}
	public void setLocationradiusfiltervalueid(int locationradiusfiltervalueid) {
		this.locationradiusfiltervalueid = locationradiusfiltervalueid;
	}
	public int getTimefiltervalueid() {
		return timefiltervalueid;
	}
	public void setTimefiltervalueid(int timefiltervalueid) {
		this.timefiltervalueid = timefiltervalueid;
	}
	public int getCostfiltervalueid() {
		return costfiltervalueid;
	}
	public void setCostfiltervalueid(int costfiltervalueid) {
		this.costfiltervalueid = costfiltervalueid;
	}
	public int getReviewfiltervalueid() {
		return reviewfiltervalueid;
	}
	public void setReviewfiltervalueid(int reviewfiltervalueid) {
		this.reviewfiltervalueid = reviewfiltervalueid;
	}
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public String getCreatedby() {
		return createdby;
	}
	public void setCreatedby(String createdby) {
		this.createdby = createdby;
	}
	public Date getCreateddate() {
		return createddate;
	}
	public void setCreateddate(Date createddate) {
		this.createddate = createddate;
	}
	public String getUpdatedby() {
		return updatedby;
	}
	public void setUpdatedby(String updatedby) {
		this.updatedby = updatedby;
	}
	public Date getUpdateddate() {
		return updateddate;
	}
	public void setUpdateddate(Date updateddate) {
		this.updateddate = updateddate;
	}
	
	
	
	
	
	
	
	
	
}
