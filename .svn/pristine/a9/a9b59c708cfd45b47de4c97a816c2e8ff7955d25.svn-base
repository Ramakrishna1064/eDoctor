package com.ensis.mediguru.models;

import java.util.ArrayList;
import java.util.List;

import com.ensis.mediguru.dto.LanguageType;

/**
 * @author Venu
 *
 */
public class PhysicianProfileModel {

	private PhysicianProfilePersonalInfoModel physicianProfilePersonalInfo;

	private List<LanguageType> languages;

	private List<PhysicianSpecialityAndQualificationsModel> specialitiesAndQualifications;

	private PhysicianFacilityPracticeLocationsModel clinicInfo;

	private ArrayList<AvailabilityTimingsModel> availabilityTimings;

	public PhysicianProfilePersonalInfoModel getPhysicianProfilePersonalInfo() {
		return physicianProfilePersonalInfo;
	}

	public void setPhysicianProfilePersonalInfo(
			PhysicianProfilePersonalInfoModel physicianProfilePersonalInfo) {
		this.physicianProfilePersonalInfo = physicianProfilePersonalInfo;
	}

	public List<LanguageType> getLanguages() {
		return languages;
	}

	public void setLanguages(List<LanguageType> languages) {
		this.languages = languages;
	}

	public List<PhysicianSpecialityAndQualificationsModel> getSpecialitiesAndQualifications() {
		return specialitiesAndQualifications;
	}

	public void setSpecialitiesAndQualifications(
			List<PhysicianSpecialityAndQualificationsModel> specialitiesAndQualifications) {
		this.specialitiesAndQualifications = specialitiesAndQualifications;
	}

	public PhysicianFacilityPracticeLocationsModel getClinicInfo() {
		return clinicInfo;
	}

	public void setClinicInfo(PhysicianFacilityPracticeLocationsModel clinicInfo) {
		this.clinicInfo = clinicInfo;
	}

	public ArrayList<AvailabilityTimingsModel> getAvailabilityTimings() {
		return availabilityTimings;
	}

	public void setAvailabilityTimings(
			ArrayList<AvailabilityTimingsModel> availabilityTimings) {
		this.availabilityTimings = availabilityTimings;
	}
}
